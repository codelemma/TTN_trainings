class Person1{
        String name;
        int age;
    Person1(String x, int y){
        name=x;
        age=y;
    }
}
class Employee{
    String name;
    int age;
    int eID;
    Employee(){
        name="xyz";
        age=20;
        eID=102;
    }
    Employee(Person1 obj){
        this.name=obj.name;
        this.age=obj.age;
        eID=101;
    }
}
public class Copy_Constructor {
    Person1 p=new Person1("abc", 50);
    System.out.println(p.name);